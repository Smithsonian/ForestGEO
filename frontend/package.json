{
  "name": "forestgeo-app",
  "version": "development",
  "private": true,
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^4.0.0",
    "popper.js": "@popperjs/core"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "rm -rf build && next lint --fix --quiet && prettier --write . --log-level silent && next build --debug",
    "start": "node build/standalone/server.js -p 3000",
    "bs": "next build; npm run start",
    "bswin": "npm run build ; npm run start",
    "bdwin": "npm run build ; npm run dev",
    "clean": "rm -rf .next node_modules build; npm install",
    "cleanwin": "del /S /Q node_modules build; npm install",
    "lint": "next lint --fix --quiet && prettier --write . --log-level silent",
    "format": "prettier --write .",
    "cy:open": "cypress open",
    "cy:open-ct": "cypress open-ct",
    "end": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "end:headless": "start-server-and-test start http://localhost:3000 \"cypress run --e2e\"",
    "com": "cypress open --component",
    "com:headless": "cypress run --component",
    "dep:orphans": "madge --extensions ts,tsx --exclude '^(pages|__tests__|cypress|scripts|sqlscripting|sampledata)' --orphans components lib utils config types",
    "clean:orphans": "npm run dep:orphans --silent | sed '1d' | xargs -I {} git rm \"{}\"",
    "dep:cycles": "madge --extensions ts,tsx --circular app components lib utils config types"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.27.0",
    "@azure/storage-queue": "^12.26.0",
